{
  "name": "assignment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/jest": "^0.2.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
